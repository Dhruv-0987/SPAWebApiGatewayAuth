{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenIdConnectOptions": {
    "Authority": "{{ REPLACED WHEN DEPLOYING }}",
    "ClientId": "spa-angular-client",
    "ResponseType": "code",
    "ResponseMode": "query",
    "Scope": [
      "openid",
      "profile",
      "email"
    ],
    "CallbackPath": "/authentication/login-callback",
    "SignedOutRedirectUri": "/signout-callback-oidc",
    "MapInboundClaims": false,
    "GetClaimsFromUserInfoEndpoint": true,
    "SaveTokens": true
  },
  "AllowedCorsOrigins": [
    "https://localhost:4200",
    "http://localhost:4200"
  ],
  "ProxyConfigOptions": {
    "Routes": [
      {
        "RouteId": "weather",
        "ClusterId": "weather-api-cluster-1",
        "Match": {
          "Path": "/api/weather-forecast/{**catch-all}"
        },
        "AuthorizationPolicy": "AuthenticatedUser"
      }
    ],
    "Clusters": [
      {
        "ClusterId": "weather-api-cluster-1",
        "Destinations": [
          {
            "DestinationId": "weather-api-cluster-1/destination1",
            "Address": "{{ REPLACED WHEN DEPLOYING }}"
          }
        ]
      }
    ]
  }
}
